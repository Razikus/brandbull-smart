{
  "expo": {
    "name": "bb-smart",
    "slug": "bb-smart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand.png",
    "scheme": "bbsmart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "eu.razniewski.bbsmart"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/brand.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "eu.razniewski.bbsmart"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/brand.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ],
      "./plugins/heiman-bluetooth-plugin",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/brand.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
